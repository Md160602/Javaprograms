package Arrayfirst;

public class Testarray 
{
	void pattern()
	{
		for(int i=0;i<5;i++)
		{
			for(int space=5;space>1+i;space--)
			{
				System.out.println(" ");
			}
			for(int j=1;j<i+1;j++)
			{
				System.out.print(j);
			}
		}
		
	}
	public static void main(String[] args) 
	{
		Testarray obj=new Testarray();
		obj.pattern();
		
	}

}
