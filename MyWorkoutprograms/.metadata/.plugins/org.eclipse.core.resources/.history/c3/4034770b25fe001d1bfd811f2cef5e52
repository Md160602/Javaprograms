package Arrayfirst;

public class Testarray 
{
	void pattern()
	{
		for(int i=6;i<1;i--)
		{
			for(int j=0;j<i;j++)
			{
				System.out.print(" ");
			}
			for(int k=0;k<i;k++)
			{
				System.out.println(k);
			}
			System.out.println();
		}
		
	}
	public static void main(String[] args) 
	{
		Testarray obj=new Testarray();
		obj.pattern();
		
	}

}
