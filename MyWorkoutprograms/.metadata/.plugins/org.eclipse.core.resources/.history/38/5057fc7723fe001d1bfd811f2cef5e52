package Arrayfirst;

public class Testarray 
{
	void pattern()
	{
		int i=5;
		while(i>0)
		{
		  int j=0;
		  while(j<5)
		  {
			  System.out.print(i+" ");
		  }
	      
	      i--;
			
		}
		
	}
	public static void main(String[] args) 
	{
		Testarray obj=new Testarray();
		obj.pattern();
		
	}

}
