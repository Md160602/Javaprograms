package Realtimepackage;

public class Studentobjects 
{
	public static void main(String[] args)
	{/*
		 * Student dev=new Student ("Mahadevan",2023001,"B.sc","Prist");
		 * System.out.println(dev); //Print the object only and hence prove overrode the
		 * object class in String method. System.out.println("----------"); Student
		 * lajeem=new Student("lajeem"); System.out.println(lajeem);
		 * lajeem.setreg_no(2023002); lajeem.getreg_no(); System.out.println(lajeem);
		 * System.out.println("----------"); Student kavin =new
		 * Student("Kavin",2023003,"B.E cs"); kavin.setcollege_name("Govt college");
		 * kavin.getcollege_name(); System.out.println(kavin);
		 * System.out.println("----------"); Student senthil=new
		 * Student("Senthil",2023004); senthil.setdept("B.E cs"); senthil.getdept();
		 * System.out.println(senthil); System.out.println("----------"); Student
		 * dinesh=new Student(); System.out.println(dinesh);
		 */
		String s="Welcome";
		String s1="WELCOME";
		String s2="Welcome";
		System.out.println(s.hashCode());
		System.out.println(s1.hashCode());
		System.out.println(s2.hashCode());
		
		Student obj1=new Student();
		Student obj2=new Student();
		System.out.println("my object hashcode is "+obj1.hashCode());
		System.out.println("my object hashcode is "+obj2.hashCode());
		String object1=new String("Devan");
		System.out.println("String object hashcode is "+object1.hashCode());
		String object2=new String("Devan");
		System.out.println("String object hashcode is "+object2.hashCode());
		System.out.println(obj1.equals(obj2));
		//boolean flag=false;
		System.out.println(obj1==obj2);
		System.out.println(s==s1);
		System.out.println(s==s2);
		System.out.println(s1==s2);
		System.out.println(s.equals(s1));
		System.out.println(s.equals(s2));
		System.out.println(s1.equals(s2));
	}

}
